    <string name="diseaseName_1">Anthracnose</string>
    <string name="diseaseSymptoms_1">Dark sunken lesions on leaves, stems, and fruits.</string>
    <string name="diseaseCause_1">Fungal (Colletotrichum).</string>
    <string name="diseaseManagement_1">Remove infected parts, use copper fungicides, and ensure proper air circulation.</string>
    <string name="diseaseName_2">Blossom-End Rot</string>
    <string name="diseaseSymptoms_2">Dark, sunken spots on the bottoms of fruits like tomatoes and squash.</string>
    <string name="diseaseCause_2">Calcium deficiency, uneven watering.</string>
    <string name="diseaseManagement_2">Test soil pH, add calcium fertilizers, and water plants consistently.</string>
    <string name="diseaseName_3">Downy Mildew</string>
    <string name="diseaseSymptoms_3">Yellowing leaves, gray spores on the undersides of leaves.</string>
    <string name="diseaseCause_3">Fungal (Peronospora).</string>
    <string name="diseaseManagement_3">Remove infected parts, prune for air circulation, and avoid overhead watering.</string>
    <string name="diseaseName_4">Powdery Mildew</string>
    <string name="diseaseSymptoms_4">White powdery growth on leaves and stems.</string>
    <string name="diseaseCause_4">Fungal (Erysiphe).</string>
    <string name="diseaseManagement_4">Apply sulfur-based fungicides, ensure proper spacing, and reduce humidity.</string>
    <string name="diseaseName_5">Black Spot</string>
    <string name="diseaseSymptoms_5">Black spots surrounded by yellowing on leaves.</string>
    <string name="diseaseCause_5">Fungal (Diplocarpon rosae).</string>
    <string name="diseaseManagement_5">Remove infected leaves, avoid overhead watering, and apply fungicides.</string>
    <string name="diseaseName_6">Rust</string>
    <string name="diseaseSymptoms_6">Orange or red pustules on the undersides of leaves.</string>
    <string name="diseaseCause_6">Fungal (Puccinia).</string>
    <string name="diseaseManagement_6">Remove infected foliage, improve ventilation, and use fungicides.</string>
    <string name="diseaseName_7">Early Blight</string>
    <string name="diseaseSymptoms_7">Brown concentric rings on leaves, often near the stem.</string>
    <string name="diseaseCause_7">Fungal (Alternaria solani).</string>
    <string name="diseaseManagement_7">Remove debris, rotate crops, and use disease-resistant varieties.</string>
    <string name="diseaseName_8">Leaf Spot</string>
    <string name="diseaseSymptoms_8">Brown or black spots with defined edges on leaves.</string>
    <string name="diseaseCause_8">Fungal or bacterial (Septoria).</string>
    <string name="diseaseManagement_8">Avoid wetting leaves during irrigation, and apply copper-based fungicides.</string>
    <string name="diseaseName_9">Root Rot</string>
    <string name="diseaseSymptoms_9">Wilting plants with brown or black rotted roots.</string>
    <string name="diseaseCause_9">Overwatering or poor drainage.</string>
    <string name="diseaseManagement_9">Improve soil drainage, reduce watering frequency, and use fungicides if caused by pathogens.</string>
    <string name="diseaseName_10">Bacterial Wilt</string>
    <string name="diseaseSymptoms_10">Sudden wilting of plants without yellowing.</string>
    <string name="diseaseCause_10">Bacterial (Ralstonia solanacearum).</string>
    <string name="diseaseManagement_10">Remove infected plants, sterilize tools, and use resistant plant varieties.</string>
    <string name="diseaseName_11">Canker</string>
    <string name="diseaseSymptoms_11">Sunken, dark lesions on stems or branches; may ooze sap.</string>
    <string name="diseaseCause_11">Fungal (Cytospora) or bacterial.</string>
    <string name="diseaseManagement_11">Prune infected areas, sterilize pruning tools, and improve plant health through proper care.</string>
    <string name="diseaseName_12">Verticillium Wilt</string>
    <string name="diseaseSymptoms_12">Wilting and yellowing of leaves, stunted growth, and browning of vascular tissue in stems.</string>
    <string name="diseaseCause_12">Fungal (Verticillium spp.).</string>
    <string name="diseaseManagement_12">Remove infected plants, rotate crops, and avoid planting susceptible species in contaminated soil.</string>
    <string name="diseaseName_13">Fusarium Wilt</string>
    <string name="diseaseSymptoms_13">Wilting leaves, yellowing along the veins, and plant death.</string>
    <string name="diseaseCause_13">Fungal (Fusarium oxysporum).</string>
    <string name="diseaseManagement_13">Use resistant plant varieties, sterilize soil, and improve drainage.</string>
    <string name="diseaseName_14">Clubroot</string>
    <string name="diseaseSymptoms_14">Swollen, misshapen roots; wilting and stunted growth.</string>
    <string name="diseaseCause_14">Pathogen (Plasmodiophora brassicae).</string>
    <string name="diseaseManagement_14">Maintain soil pH above 7.0, remove infected plants, and rotate crops with non-host species.</string>
    <string name="diseaseName_15">Botrytis Blight</string>
    <string name="diseaseSymptoms_15">Gray mold on flowers, fruits, and leaves; soft and rotted tissue.</string>
    <string name="diseaseCause_15">Fungal (Botrytis cinerea).</string>
    <string name="diseaseManagement_15">Prune affected parts, improve air circulation, and avoid water splashing on plants.</string>
    <string name="diseaseName_16">Scab</string>
    <string name="diseaseSymptoms_16">Corky, raised lesions on fruits, leaves, or tubers (e.g., apple scab).</string>
    <string name="diseaseCause_16">Fungal (Venturia inaequalis).</string>
    <string name="diseaseManagement_16">Use disease-resistant varieties, apply fungicides, and remove fallen debris.</string>
    <string name="diseaseName_17">Bacterial Leaf Spot</string>
    <string name="diseaseSymptoms_17">Dark water-soaked spots on leaves that may dry and crack.</string>
    <string name="diseaseCause_17">Bacterial (Xanthomonas or Pseudomonas).</string>
    <string name="diseaseManagement_17">Use disease-free seeds, avoid overhead watering, and apply copper-based fungicides.</string>
    <string name="diseaseName_18">Damping-Off</string>
    <string name="diseaseSymptoms_18">Seedlings collapse at soil level, turning brown or mushy.</string>
    <string name="diseaseCause_18">Fungal (Pythium or Rhizoctonia).</string>
    <string name="diseaseManagement_18">Use sterile potting mix, avoid overwatering, and ensure proper drainage.</string>
    <string name="diseaseName_19">Mosaic Virus</string>
    <string name="diseaseSymptoms_19">Mottled yellow, green, or white patterns on leaves; stunted growth.</string>
    <string name="diseaseCause_19">Viral (Tobacco Mosaic Virus, etc.).</string>
    <string name="diseaseManagement_19">Remove infected plants, control insect vectors, and use resistant plant varieties.</string>
    <string name="diseaseName_20">Crown Gall</string>
    <string name="diseaseSymptoms_20">Tumor-like growths at the base of stems or roots.</string>
    <string name="diseaseCause_20">Bacterial (Agrobacterium tumefaciens).</string>
    <string name="diseaseManagement_20">Avoid wounding plants, remove infected plants, and sanitize tools.</string>
    <string name="diseaseName_21">Late Blight</string>
    <string name="diseaseSymptoms_21">Water-soaked spots on leaves that turn brown and spread rapidly; fruits may rot.</string>
    <string name="diseaseCause_21">Fungal (Phytophthora infestans).</string>
    <string name="diseaseManagement_21">Use resistant varieties, rotate crops, and apply fungicides as needed.</string>
    <string name="diseaseName_22">Bacterial Blight</string>
    <string name="diseaseSymptoms_22">Dark, water-soaked lesions on leaves and stems that may ooze bacterial exudates.</string>
    <string name="diseaseCause_22">Bacterial (Xanthomonas spp.).</string>
    <string name="diseaseManagement_22">Avoid overhead irrigation, remove infected plants, and apply copper-based sprays.</string>
    <string name="diseaseName_23">Fire Blight</string>
    <string name="diseaseSymptoms_23">Wilting leaves and shoots that appear scorched; bacterial ooze may form on branches.</string>
    <string name="diseaseCause_23">Bacterial (Erwinia amylovora).</string>
    <string name="diseaseManagement_23">Prune infected areas, sterilize tools, and avoid planting highly susceptible species together.</string>
    <string name="diseaseName_24">Phytophthora Root Rot</string>
    <string name="diseaseSymptoms_24">Yellowing leaves, stunted growth, and root decay.</string>
    <string name="diseaseCause_24">Fungal-like pathogen (Phytophthora).</string>
    <string name="diseaseManagement_24">Improve soil drainage, avoid waterlogging, and treat with appropriate fungicides.</string>
    <string name="diseaseName_25">Septoria Leaf Spot</string>
    <string name="diseaseSymptoms_25">Small, dark spots with yellow halos on leaves; leads to premature leaf drop.</string>
    <string name="diseaseCause_25">Fungal (Septoria lycopersici).</string>
    <string name="diseaseManagement_25">Remove infected leaves, avoid wetting foliage, and apply fungicides if necessary.</string>
    <string name="diseaseName_26">Sooty Mold</string>
    <string name="diseaseSymptoms_26">Black, powdery coating on leaves and stems caused by fungal growth on honeydew from insects.</string>
    <string name="diseaseCause_26">Fungi on insect secretions (e.g., aphids).</string>
    <string name="diseaseManagement_26">Control insect populations, wash leaves, and improve plant health.</string>
    <string name="diseaseName_27">White Mold</string>
    <string name="diseaseSymptoms_27">White, cottony fungal growth on stems and leaves; often causes plant collapse.</string>
    <string name="diseaseCause_27">Fungal (Sclerotinia sclerotiorum).</string>
    <string name="diseaseManagement_27">Remove infected plants, avoid overcrowding, and improve air circulation.</string>
    <string name="diseaseName_28">Apple Scab</string>
    <string name="diseaseSymptoms_28">Olive-green or black velvety spots on apples and leaves.</string>
    <string name="diseaseCause_28">Fungal (Venturia inaequalis).</string>
    <string name="diseaseManagement_28">Use resistant varieties, remove fallen leaves, and apply fungicides.</string>
    <string name="diseaseName_29">Peach Leaf Curl</string>
    <string name="diseaseSymptoms_29">Distorted, reddened, or thickened leaves; premature leaf drop.</string>
    <string name="diseaseCause_29">Fungal (Taphrina deformans).</string>
    <string name="diseaseManagement_29">Apply fungicides during dormancy, and remove infected leaves.</string>
    <string name="diseaseName_30">Cercospora Leaf Spot</string>
    <string name="diseaseSymptoms_30">Circular spots with gray centers and dark edges on leaves; leads to defoliation.</string>
    <string name="diseaseCause_30">Fungal (Cercospora spp.).</string>
    <string name="diseaseManagement_30">Use resistant varieties, remove plant debris, and apply fungicides.</string>
